{
  "Syntax Frame File Header": {
    "scope": "javascript,typescript",
    "prefix": "fheader",
    "description": "Insert Syntax Frame file header",
    "body": [
      "// ================================================================",
      "// FRAME   : Syntax Frame",
      "// FILE    : ${TM_FILENAME}",
      "// PATH    : ${TM_DIRECTORY}",
      "// AUTHOR  : ${1:Author Name}",
      "// CREATED : ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      "// MODIFIED: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      "// ----------------------------------------------------------------",
      "// DESCRIPTION:",
      "// ${2:Short description of this file}",
      "//",
      "// RESPONSIBILITY:",
      "// - ${3:Primary responsibility of this file}",
      "// ================================================================",
      ""
    ]
  }
  ,
"Syntax Frame Section": {
  "scope": "javascript,typescript",
  "prefix": "section",
  "description": "Insert a major Syntax Frame section",
  "body": [
    "// ================================================================",
    "// SECTION: ${1:Section Title}",
    "// ================================================================",
    ""
  ]
},
"Syntax Frame Subsection": {
  "scope": "javascript,typescript",
  "prefix": "subsec",
  "description": "Insert a Syntax Frame subsection",
  "body": [
    "// ------------------------------------------------",
    "// SUBSECTION: ${1:Subsection Title}",
    "// ------------------------------------------------",
    ""
  ]
},
"Syntax Frame End Section": {
  "scope": "javascript,typescript",
  "prefix": "end",
  "description": "End a Syntax Frame section",
  "body": [
    "// ================================================================",
    "// END SECTION: ${1:Section Title}",
    "// ================================================================",
    ""
  ]
}
,
"Syntax Frame Function Doc (Detailed)": {
  "scope": "javascript,typescript",
  "prefix": "fdoc",
  "description": "Detailed function documentation (Syntax Frame)",
  "body": [
    "/**",
    " * FUNCTION: ${1:functionName}",
    " * DESCRIPTION:",
    " * ${2:Explain what this function does}",
    " *",
    " * PARAMETERS:",
    " * @param {${3:type}} ${4:name} - ${5:description}",
    " *",
    " * RETURNS:",
    " * @returns {${6:type}} ${7:description}",
    " *",
    " * NOTES:",
    " * ${8:Any important notes}",
    " */"
  ]
},
"Syntax Frame Function Doc (Simple)": {
  "scope": "javascript,typescript",
  "prefix": "fds",
  "description": "Simple function documentation (Syntax Frame)",
  "body": [
    "/**",
    " * ${1:Brief description of the function}",
    " */"
  ]
}
,
"Syntax Frame TODO": {
  "scope": "javascript,typescript",
  "prefix": "todo",
  "description": "Task marker (TODO)",
  "body": [
    "// TODO: ${1:Describe task} [${2:owner}, ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}]"
  ]
},
"Syntax Frame FIXME": {
  "scope": "javascript,typescript",
  "prefix": "fixme",
  "description": "Fix required marker (FIXME)",
  "body": [
    "// FIXME: ${1:Describe issue} [${2:owner}, ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}]"
  ]
},
"Syntax Frame SECURITY": {
  "scope": "javascript,typescript",
  "prefix": "security",
  "description": "Security marker",
  "body": [
    "// SECURITY: ${1:Describe risk} [${2:owner}, ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}]"
  ]
}
,
"Syntax Frame Debug Log": {
  "scope": "javascript,typescript",
  "prefix": "debug",
  "description": "Debug log (Syntax Frame)",
  "body": [
    "console.log('[DEBUG]', ${1:variable});"
  ]
},
"Syntax Frame Performance Block": {
  "scope": "javascript,typescript",
  "prefix": "perf",
  "description": "Performance measurement block (Syntax Frame)",
  "body": [
    "const ${1:label}Start = performance.now();",
    "",
    "${2:// code to measure}",
    "",
    "const ${1:label}End = performance.now();",
    "console.log('[PERF] ${1:label}:', (${1:label}End - ${1:label}Start).toFixed(2), 'ms');"
  ]
},
"Syntax Frame Console Group": {
  "scope": "javascript,typescript",
  "prefix": "cgroup",
  "description": "Console log group (Syntax Frame)",
  "body": [
    "console.group('${1:Group Name}');",
    "${2:// grouped logs}",
    "console.groupEnd();"
  ]
}

}
